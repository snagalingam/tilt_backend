{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "snagalingam/tilt-app",
      "hostname": "app",
      "essential": true,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "snagalingam/tilt-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["app"],
      "memory": 128
    }
  ]
}
