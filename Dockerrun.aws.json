{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "614818178581.dkr.ecr.us-east-2.amazonaws.com/tilt-app-staging:latest",
      "hostname": "backend",
      "essential": true,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "614818178581.dkr.ecr.us-east-2.amazonaws.com/tilt-nginx-staging:latest",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["backend"],
      "memory": 128
    }
  ]
}
