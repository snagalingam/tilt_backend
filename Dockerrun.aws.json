{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "app",
      "host": {
        "sourcePath": "/var/app/current/app"
      }
    },
    {
      "name": "nginx-proxy-conf",
      "host": {
        "sourcePath": "/var/app/current/conf.d"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "app",
      "essential": true,
      "image": "614818178581.dkr.ecr.us-east-2.amazonaws.com/tilt-app-staging:latest",
      "hostname": "app",
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-app",
          "containerPath": "/var/log/containers/app"
        },
        {
          "sourceVolume": "app",
          "containerPath": "/home/app/",
          "readOnly": false
        }
      ]
    },
    {
      "name": "nginx",
      "image": "nginx",
      "essential": true,
      "hostname": "nginx",
      "links": ["app"],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "app",
          "containerPath": "/home/app/",
          "readOnly": true
        },
        {
          "sourceVolume": "awseb-logs-nginx",
          "containerPath": "/var/log/nginx"
        },
        {
          "sourceVolume": "nginx-proxy-conf",
          "containerPath": "/etc/nginx/conf.d",
          "readOnly": true
        }
      ]
    }
  ]
}
